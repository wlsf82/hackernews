{
  "name": "hackernews",
  "version": "0.1.0",
  "dependencies": {
    "axios": "^0.21.2",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run ct && npm run vt && npm run e2e:ci",
    "e2e": "cypress run --record --tag 'e2e'",
    "e2e:ci": "start-server-and-test http://localhost:3000 e2e",
    "e2e:prod": "cypress run --record --tag 'e2e prod' --config baseUrl=https://hackernews-seven.vercel.app",
    "ct": "cypress run --component --record --tag 'component' --env grepTags=-@visual",
    "cy:open": "cypress open",
    "vt": "percy exec -- cypress run --component --record --tag 'component,visual' --env grepTags=@visual"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "devDependencies": {
    "@cypress/grep": "^3.1.5",
    "@percy/cli": "^1.24.0",
    "@percy/cypress": "^3.1.2",
    "cypress": "^12.11.0",
    "start-server-and-test": "^2.0.0"
  }
}
