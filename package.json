{
  "name": "hackernews",
  "version": "0.1.0",
  "dependencies": {
    "axios": "^1.6.7",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "e2e": "cypress run --record --tag 'e2e'",
    "e2e:prod": "cypress run --record --tag 'e2e prod' --config baseUrl=https://hackernews-seven.vercel.app",
    "ct": "cypress run --component --record --tag 'component' --env grepTags=-@visual",
    "cy:open": "cypress open",
    "vt": "percy exec -- cypress run --component --record --tag 'component,visual' --env grepTags=@visual"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "devDependencies": {
    "@cypress/grep": "^4.0.1",
    "@percy/cli": "^1.28.0",
    "@percy/cypress": "^3.1.2",
    "cypress": "^13.6.4",
    "start-server-and-test": "^2.0.3"
  }
}
